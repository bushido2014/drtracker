<div{{ attributes.addClass('link-icon') }}>
  {% if not label_hidden %}
    <h3{{ title_attributes }}>{{ label }}</h3>
  {% endif %}

  {% for item in items %}
    <a href="{{ item.content['#url'] }}" class="btn btn-primary d-flex">
    <span>  {{ item.content['#title'] }} </span>
    <span><i class="fa-solid fa-arrow-right"></i></span>
    </a>
  {% endfor %}
</div>
