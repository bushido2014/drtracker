<section class="partnership">
<div class="container">

 <div class="section-title">
    <h2 class="heading-title">{{paragraph.field_title.value}}</h2>
   <div class="description">
      {{paragraph.field_description.value}}
    </div>
</div>

<div class="career-images d-grid">
  {% for media in paragraph.field_images %} {% set file =
        media.entity.field_media_image.entity %}
        <figure class="career-img">
          <img
            src="{{ file_url(file.uri.value) }}"
             alt="{{ media.field_media_image.alt }}"
            loading="lazy"
          />
        </figure>
        {% endfor %}
</div>





</div>
</section>
